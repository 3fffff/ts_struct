{
    "version": "1.0.0",
    "configurations": [
      {
        "name": "TS-Node",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/out/src/index.js",
        "cwd": "${workspaceRoot}",
        "internalConsoleOptions": "openOnSessionStart",
        "skipFiles": ["<node_internals>/**", "node_modules/**"],
        "sourceMaps": true,
        "preLaunchTask": "build",
      }
    ]
  }
  
  